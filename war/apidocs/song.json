{
  "apiVersion" : "v1",
  "swaggerVersion" : "1.2",
  "basePath" : "http://host:port/apis",
  "resourcePath" : "/song",
  "produces" : [ "application/json" ],
  "apis" : [ {
    "path" : "/song/{id}",
    "operations" : [ {
      "method" : "GET",
      "summary" : "Find a song by ID",
      "notes" : "",
      "type" : "FaveItem",
      "nickname" : "getSong",
      "authorizations" : { },
      "parameters" : [ {
        "name" : "id",
        "description" : "ID of song to be fetched",
        "required" : true,
        "type" : "string",
        "paramType" : "path"
      } ]
    } ]
  }, {
    "path" : "/song/{id}/favelists",
    "operations" : [ {
      "method" : "GET",
      "summary" : "Get a list of users who have this song in their FaveList",
      "notes" : "",
      "type" : "UserListResultCollection",
      "nickname" : "getFaveLists",
      "authorizations" : { },
      "parameters" : [ {
        "name" : "id",
        "description" : "The song ID",
        "required" : true,
        "type" : "string",
        "paramType" : "path"
      } ]
    } ]
  }, {
    "path" : "/song/youtubeSearchResults",
    "operations" : [ {
      "method" : "GET",
      "summary" : "Find YouTube videos for a song",
      "notes" : "",
      "type" : "YouTubeSearchResultCollection",
      "nickname" : "getYouTubeResults",
      "authorizations" : { },
      "parameters" : [ {
        "name" : "song",
        "description" : "The song title",
        "required" : true,
        "type" : "string",
        "paramType" : "query"
      }, {
        "name" : "artist",
        "description" : "The song artist",
        "required" : true,
        "type" : "string",
        "paramType" : "query"
      } ]
    } ]
  } ],
  "models" : {
    "FaveItem" : {
      "id" : "FaveItem",
      "description" : "",
      "required" : [ "song" ],
      "properties" : {
        "song" : {
          "type" : "string",
          "description" : "The song name"
        },
        "artist" : {
          "type" : "string"
        },
        "songID" : {
          "type" : "string"
        },
        "whyline" : {
          "type" : "string"
        },
        "id" : {
          "type" : "string"
        }
      }
    },
    "YouTubeSearchResultCollection" : {
      "id" : "YouTubeSearchResultCollection",
      "properties" : {
        "items" : {
          "type" : "array",
          "items" : {
            "$ref" : "YouTubeSearchResult"
          }
        }
      }
    },
    "UserListResultCollection" : {
      "id" : "UserListResultCollection",
      "properties" : {
        "items" : {
          "type" : "array",
          "items" : {
            "$ref" : "UserListResult"
          }
        }
      }
    },
    "YouTubeSearchResult" : {
      "id" : "YouTubeSearchResult",
      "properties" : {
        "videoId" : {
          "type" : "string"
        },
        "thumbnail" : {
          "type" : "string"
        }
      }
    },
    "UserListResult" : {
      "id" : "UserListResult",
      "properties" : {
        "listName" : {
          "type" : "string"
        },
        "userName" : {
          "type" : "string"
        },
        "avatar" : {
          "type" : "string"
        }
      }
    }
  }
}