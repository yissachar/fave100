<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'
	xmlns:w="urn:import:com.fave100.client.widgets"
	xmlns:pf='urn:import:com.fave100.client.pagefragments.footer'
	ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	ui:generateLocales='default'>
	
	<ui:style>	
	</ui:style>

	<g:HTMLPanel ui:styleName="container">
		<g:HTMLPanel ui:field="topBar"/>		
		<div class="content">
		    <g:HTMLPanel ui:field="faveFeed"/>
			<div class="faveListContainer">
				<h2 class="itemHeader">The Fave100</h2>
				<w:NonpersonalFaveList ui:field="masterFaveList"/>
			</div>
		</div>	
		<pf:Footer/>
	</g:HTMLPanel>	
