<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	
	<ui:style type="com.fave100.client.pages.song.widgets.whyline.WhylineWaterfall.WhylineWaterfallStyle">
		.container {
			display: block;
			margin: 0 auto;
			width: 320px;
			float: left;
			border: 1px solid #555;
			border-left: 0px;
			height: 508px;
			overflow-y: auto;
		}
		
		.noWhyline {
			padding: 6px;
			font-size: 1.5em;
		}
	</ui:style>
	
	<g:VerticalPanel ui:field="container" ui:styleName="{style.container}"/>
	
</ui:UiBinder> 