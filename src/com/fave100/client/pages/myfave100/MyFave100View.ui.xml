<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'
	xmlns:d="urn:import:com.fave100.client.pages.myfave100"
	xmlns:pf='urn:import:com.fave100.client.pagefragments.footer'
	ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	ui:generateLocales='default'>

	<ui:style>	
	</ui:style>
	
	<ui:with type="com.fave100.client.place.NameTokens" field="nameTokens"/>
	<g:HTMLPanel ui:styleName="container">
		<g:HTMLPanel ui:field="topBar"/>
		<div class="content">
			<g:SuggestBox ui:field="songSuggestBox"/>
			<g:InlineHyperlink targetHistoryToken="{nameTokens.getSearch}">Advanced search</g:InlineHyperlink>
			<d:PersonalFaveList ui:field="personalFaveList"/>			
			<g:HTMLPanel ui:field="faveFeed"/>
		</div>	
		<pf:Footer/>
	</g:HTMLPanel>
</ui:UiBinder>
