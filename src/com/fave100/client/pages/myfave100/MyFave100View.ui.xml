<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'
	xmlns:d="urn:import:com.fave100.client.pages.myfave100"
	xmlns:pf='urn:import:com.fave100.client.pagefragments.footer'
	ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	ui:generateLocales='default'>

	<ui:style>
		@external .faveList;
		.faveList{}			
	</ui:style>
	<g:HTMLPanel>
		<g:HTMLPanel ui:field="topBar"/>		
		<g:SuggestBox ui:field="songSuggestBox"/>
		<d:PersonalFaveList ui:field="personalFaveList" styleName="{style.faveList}"/>
		<pf:Footer/>
	</g:HTMLPanel>
</ui:UiBinder>
