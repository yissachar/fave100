<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'
	xmlns:pf='urn:import:com.fave100.client.pagefragments.footer'
	ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	ui:generateLocales='default'>

	<g:HTMLPanel ui:styleName="container">
		<g:HTMLPanel ui:field="topBar"/>
		<div class="content">
			<h4>Profile</h4>			
			<g:FormPanel method="POST" encoding="multipart/form-data" ui:field="profileForm">
				<g:HTMLPanel styleName="registerContainer profileForm">
					<g:Label>Email</g:Label>
					<g:TextBox  ui:field="emailInput"/>
					<g:Label>Avatar</g:Label>
					<g:FileUpload name="avatar" />
					<g:SubmitButton>Save</g:SubmitButton>
				</g:HTMLPanel>
			</g:FormPanel>
		</div>
		<pf:Footer/>
	</g:HTMLPanel>
</ui:UiBinder>
