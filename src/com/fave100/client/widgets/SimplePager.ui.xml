<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.pageLinks a, .pageLinks div, .previousLink, .nextLink{
			margin-top: 6px;
			margin-right: 6px;
			float: left;
			cursor: pointer;
		}
		
		.pageLinks div {
			font-weight: bold;
		}
	</ui:style>
	<g:HTMLPanel ui:styleName="row">
		<g:Anchor ui:field="previousLink" ui:styleName="{style.previousLink}">Previous</g:Anchor>
		<g:HTMLPanel ui:field="pageLinks" ui:styleName="{style.pageLinks}"/>
		<g:Anchor ui:field="nextLink" ui:styleName="{style.nextLink}">Next</g:Anchor>		
	</g:HTMLPanel>
</ui:UiBinder> 