<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'
	ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	ui:generateLocales='default'>

	<ui:style>
		
	</ui:style>
	
	<ui:with field="img" type="com.fave100.client.resources.img.ImageResources" />
	
	<g:PopupPanel>
		<g:HTMLPanel ui:field="loginLightBox">
			<g:FocusPanel ui:field="lightBoxBackground" styleName="lightBoxBackground"/>			
			<g:HTMLPanel styleName="loginLightBox">
				<div class="lightBoxHeader">
					<g:Image resource="{img.logo}"/>
				</div>
				<g:HTMLPanel ui:field="loginContainer" ui:styleName="innerLoginBox"/>
			</g:HTMLPanel>
		</g:HTMLPanel>		
	</g:PopupPanel>
</ui:UiBinder>
